{
  "project": {
    "version": "1.0.0-beta.21",
    "name": "fundabitat-ng2"
  },
  "apps": [
    {
      "assets": ["assets"],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.dev.ts",
        "prod": "environments/environment.prod.ts"
      },
      "index": "index.html",
      "main": "main.ts",
      "mobile": false,
      "outDir": "dist",
      "prefix": "app",
      "root": "src",
      "scripts": [
        "../node_modules/hammerjs/hammer.js"
      ],
      "styles": [
        "../node_modules/flexlay/dist/flexlay.css",
        "styles.scss"
      ],
      "test": "test.ts",
      "tsconfig": "tsconfig.json"
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
